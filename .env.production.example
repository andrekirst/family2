# Family Hub Production Environment Variables
# Copy this file to .env and fill in your values

# Database Configuration
POSTGRES_DB=familyhub
POSTGRES_USER=familyhub
POSTGRES_PASSWORD=YourSecurePasswordHere123!

# JWT Configuration (CHANGE THIS!)
JWT_SECRET=your-secret-key-must-be-at-least-32-characters-long-change-this-to-random-string

# SMTP Configuration (Optional - for sending emails)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
FROM_EMAIL=noreply@familyhub.local
FROM_NAME=Family Hub
